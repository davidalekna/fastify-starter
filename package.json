{
  "name": "test",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --quiet",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc -b",
    "prepare": "husky install",
    "test": "tap --node-arg=\"-r\" --node-arg=\"ts-node/register\"",
    "test:watch": "tap --reporter=list --watch",
    "test:watch:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --watchAll --runInBand",
    "test:coverage": "npm run test -- --coverage",
    "format": "prettier --write \"**/*.+(ts|js|json|md|mdx)\"",
    "lint": "echo setup eslint",
    "validate": "npm-run-all --parallel test:coverage lint"
  },
  "devDependencies": {
    "@types/faker": "^5.5.6",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.2",
    "@types/supertest": "^2.0.11",
    "@types/tap": "^15.0.2",
    "faker": "^5.5.3",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "jest-watch-typeahead": "^0.6.4",
    "nodemon": "^2.0.5",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^5.0.2",
    "prettier": "^2.3.1",
    "rimraf": "^3.0.2",
    "tap": "^15.0.9",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tslib": "^2.2.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "fastify": "^3.17.0",
    "fastify-cors": "^6.0.1",
    "fastify-swagger": "^4.7.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate"
    }
  }
}
